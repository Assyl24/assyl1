public class HealthcareManagementSystem {

    public static void main(String[] args) {
        Patient patient = new Patient("John Doe", 30);
        System.out.println("Patient: " + patient.getName() + ", Age: " + patient.getAge());
    }
}

class Patient {
    private String name;
    private int age;

    public Patient(String name, int age) {
        this.name = name;
        this.age = age;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public int getAge() {
        return age;
    }

    public void setAge(int age) {
        this.age = age;
    }
}
